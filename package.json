{
  "name": "maintainer-bot",
  "version": "1.0.0",
  "private": true,
  "description": "A bot to automate and maintain your GitHub project!",
  "author": "Samuel Corsi-House <chouse.samuel@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com/Xenfo/maintainer-bot.git",
  "homepage": "https://github.com/Xenfo/maintainer-bot",
  "bugs": "https://github.com/Xenfo/maintainer-bot/issues",
  "keywords": [
    "github",
    "bot",
    "automation"
  ],
  "main": "bin/app.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node bin/app.js",
    "single": "npm run build && node bin/commands/process-single.js",
    "many": "npm run build && node bin/commands/process-many.js",
    "single-info": "npm run build && node bin/commands/show-info.js",
    "single-info-debug": "npm run build && node --inspect-brk bin/commands/show-info.js",
    "update-test-data": "npm run build && node bin/commands/update-test-data.js",
    "cleanproject": "npm run build && node bin/commands/clean-columns.js",
    "create-fixture": "npm run build && node bin/commands/create-fixture.js",
    "update-all-fixtures": "npm run build && node bin/commands/update-all-fixtures.js",
    "graphql-schema": "npx apollo client:codegen schema --target typescript --globalTypesFile=src/graphql/graphql-global-types.ts",
    "test": "jest"
  },
  "dependencies": {
    "@graphql-codegen/fragment-matcher": "^2.0.0",
    "@octokit/graphql-schema": "^10.1.0",
    "@octokit/webhooks": "^7.15.1",
    "@types/express": "^4.17.8",
    "@types/jest": "26.0.15",
    "@types/node": "latest",
    "@types/node-fetch": "^2.5.7",
    "@types/prettyjson": "^0.0.29",
    "@types/request": "^2.48.5",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.2",
    "definitelytyped-header-parser": "github:Microsoft/definitelytyped-header-parser#production",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.15.0",
    "express": "^4.17.1",
    "fast-json-patch": "^3.0.0-1",
    "fs-extra": "^9.0.1",
    "graceful-fs": "^4.2.4",
    "graphql": "^14.5.8",
    "idembot": "^0.0.12",
    "moment": "^2.27.0",
    "node-fetch": "^2.6.1",
    "prettyjson": "^1.2.1",
    "request": "^2.88.2",
    "smee-client": "^1.2.2",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "apollo": "^2.28.3",
    "eslint": "^7.12.1",
    "jest": "26.6.3",
    "jest-file-snapshot": "^0.5.0",
    "nock": "^13.0.4",
    "ts-jest": "^26.1.1"
  },
  "engines": {
    "node": ">= 10.13.0"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
